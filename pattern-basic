#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    // std::cout << "Hello world!";
    
    for (int i=0;i<5;i++){
        // cout << i ;
      //row
      for(int j=0;j<5;j++){
           //cols
           cout << 4-j << " ";
        }
        cout <<"\n";
        }
    return 0;
}

//patter 2

int main() {
    int n;
    cin >> n;
    //cout << n;
    
    int i = 1;
    
    int count =1; 
    while(i<=n){
        //row;
        int j=1;
        while(j<=n){
            cout << count << " ";
            count++;
            j++;
        }       
        cout << "\n";
        i++;
    }
    return 0;
}


//star patter
* 
* * 
* * * 
* * * * 
* * * * *

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;
    //cout << n;
    
    int i = 1;
    
    
    while(i<=n){
        //row;
        int j=1;
        while(j<=i){
            cout << "*" << " ";
            j++;
        }       
        cout << "\n";
        i++;
    }
    return 0;
}
