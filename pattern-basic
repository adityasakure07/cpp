#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    // std::cout << "Hello world!";
    
    for (int i=0;i<5;i++){
        // cout << i ;
      //row
      for(int j=0;j<5;j++){
           //cols
           cout << 4-j << " ";
        }
        cout <<"\n";
        }
    return 0;
}

//patter 2

int main() {
    int n;
    cin >> n;
    //cout << n;
    
    int i = 1;
    
    int count =1; 
    while(i<=n){
        //row;
        int j=1;
        while(j<=n){
            cout << count << " ";
            count++;
            j++;
        }       
        cout << "\n";
        i++;
    }
    return 0;
}


//star patter
* 
* * 
* * * 
* * * * 
* * * * *

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;
    //cout << n;
    
    int i = 1;
    
    
    while(i<=n){
        //row;
        int j=1;
        while(j<=i){
            cout << "*" << " ";
            j++;
        }       
        cout << "\n";
        i++;
    }
    return 0;
}


//pattern 
5
1 
2 1 
3 2 1 
4 3 2 1 
5 4 3 2 1 


int main() {
    int n;
    cin >> n;
    //cout << n;
    int count = 1;
    int i = 1;
    
    
    while(i<=n){
        //row;
        int j=1;
        //count = i;
        while(j<=i){
            cout << i-j+1<< " ";
            j++;
            //count++;
        }       
        cout << "\n";
        i++;
    }
    return 0;
}

//pattern
    *
   **
  ***
 ****
*****
int main() {
    int n = 5;
    cin >> n;
   //cout << n;
    int count = 1;
    int row = 1;
    char val = 'A';
    
    
    while(row <=n){
        int space = n - row; 
        // print space 
        while(space){
            cout << " ";
            space --;
        }
        
        
        //print star
        int col=1;
        //count = i;
        while(col<=row){
            // char ch = 'A' + i + j -2;
            //char ch = 'A' + i -1;
            cout << "*";
            col++;
            //count++;
        }       
        cout << "\n";
        row ++;
    }
    return 0;
}
